config {
  type: "table",
  schema: dataform.projectConfig.vars.schema,
  tags: ["ga4","mart","sessions"],
  bigquery: {
    partitionBy: "date",
    clusterBy: ["site_name","pseud_id","session_id"]
  }
}

SELECT
  date,
  site_name,
  session_time,
  pseud_id,
  session_id,
  country,
  device,
  platform,
  user_type,
  channel_grouping,
  session_source,
  session_medium,
  session_campaign,
  bounces,
  page_view,
  user_signup,
  engaged_session
FROM ${ref("int_sessions")}
